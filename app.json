{
  "expo": {
    "name": "Peri Tracker",
    "slug": "peri-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E8D5E0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.peritracker.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E8D5E0"
      },
      "package": "com.peritracker.app"
    },
    "plugins": [
      "expo-sqlite",
      "expo-asset",
      "expo-font"
    ]
  }
}
